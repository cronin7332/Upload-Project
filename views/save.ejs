<body>
    <div class="head">
        <ul>  
          <li><a href="/">Home</a></li> 
          <li><a href="/index">Upload File</a></li>
          <li><a href="/fetch">Fetch File</a></li>
          <li><a href="/list">List</a></li>
        </ul>
      </div>
      <div class="container mk">
        <!-- <button class="btn btn-primary gaps" id="btn">Copy</button> -->
        <img class="" src="/img/logo.jpeg" width="100px" height="100px" alt="ok"><br><br>
        <span for="">Your File is Uploaded Successfully.</span><br>
        <span>Your ID: <span id="val"><%= sendMs %></span><span><button class="btn btn-primary gaps" id="btn">Copy</button><br><br>           
      </div>
      <script>  
        var st = 1;
        var bt = document.getElementById("btn");
        var vals = document.getElementById("val");
        bt.addEventListener("click",function(){
            if(st===1){
                bt.innerText = "Copied";
                navigator.clipboard.writeText(vals.innerText);
            }
        })
      </script>
</body>
